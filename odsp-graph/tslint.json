{
  "rules": {
    "max-line-length": [
      false,
      140
    ],
    "typedef": false,
    "no-any": false,
    "no-null-keyword": false,
    "variable-name": false,
    "export-name": false,
    "semicolon": false,
    "no-unused-imports": false,
    "typedef-whitespace": false,
    "no-internal-module": false,
    "trailing-comma": false,
    "member-access": false,
    "member-ordering": false,
    "prefer-const": false,
    "no-bitwise": false,
    "quotemark": false,
    "no-empty": false,
    "whitespace": false,
    "triple-equals": false
  }
}