{
  "commonFolder": "common",
  "npmVersion": "3.10.8",
  "npmxMinimumVersion": "1.0.10",
  "nodeSupportedVersionRange": ">=4.3.0 <7.0.0",
  "projectFolderMinDepth": 1,
  "useLocalNpmCache": true,

  // Projects should be in the same alphabetical order that they appear
  // in Windows Explorer
  "projects": [
    // "apps" folder (alphabetical order)
    {
      "packageName": "@ms/odsp-datasources",
      "projectFolder": "odsp-datasources",
      "shouldPublish": true
    },
    {
      "packageName": "@ms/odsp-shared-react",
      "projectFolder": "odsp-shared-react",
      "shouldPublish": true
    },
    {
      "packageName": "@ms/odsp-utilities",
      "projectFolder": "odsp-utilities",
      "shouldPublish": true
    }
  ]
}
