{
  "rules": {
    "max-line-length": [
      false,
      140
    ],
    "typedef": false,
    "no-any": false,
    "no-null-keyword": false,
    "variable-name": false,
    "export-name": false,
    "prefer-const": false,
    "no-trailing-whitespace": false,
    "no-unused-imports": false,
    "quotemark": false,
    "semicolon": false
  }
}