{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
      "stopOnEntry": false,
      "args": [
        "bundle"
      ],
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": null,
      "runtimeArgs": [
        "--nolazy",
        "--debug"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "externalConsole": false,
      "sourceMaps": false,
      "outDir": null
    },
    {
      "name": "Attach",
      "type": "node",
      "request": "attach",
      "port": 5858
    },
    {
        "name": "Debug Code",
        "type": "chrome",
        "request": "attach",
        "url": "http://localhost:4322/index.html#/horizontalNav",
        "webRoot": "${workspaceRoot}/src",
        "port": 9222,
        "sourceMaps": true
    },
    {
        "name": "Edge",
        "type": "edge",
        "request": "launch",
        "url": "http://localhost:4322/index.html#/horizontalNav",
        "webRoot": "${workspaceRoot}/src",
        "sourceMaps": true
    }
  ]
}